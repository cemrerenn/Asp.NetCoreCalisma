@using EntityLayer.Concrete
@model List<Adress>

<h3 class="tittle">Bize Ulaşın</h3>
<p class="heading-btm-w3ls">Aşağıdaki form üzerinden mail göndere bilirsiniz.</p>
@foreach (var item in Model)
{
    <div class="col-md-3 contact-left">
        <h4>Adresimiz</h4>
        <p>@item.Desctiption1</p>
        <p>@item.Desctiption3</p>
        <p>@item.Desctiption2</p>
        <h4>Telefon Numaramız</h4>
        <p>@item.Desctiption4</p>
    </div>
}