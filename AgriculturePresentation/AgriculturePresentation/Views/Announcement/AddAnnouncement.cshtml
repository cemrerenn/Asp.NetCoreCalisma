@model EntityLayer.Concrete.Annouoncement
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni Yeni Duyuru Girişi</h4>

            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label >Başlık</label>
                    <input type="text" class="form-control" id="exampleInputUsername1" asp-for="Title" placeholder="Başlık Giriniz">
                </div>
                <div class="form-group">
                    <label >Açıklama</label>
                    <textarea cols="30" rows="5" asp-for="Description" placeholder="Açıklama Giriniz" class="form-control" placeholder="Açıklama Giriniz"></textarea>
                </div>
               

                <button type="submit" class="btn btn-primary me-2">Kaydet</button>

            </form>
        </div>
    </div>
</div>


